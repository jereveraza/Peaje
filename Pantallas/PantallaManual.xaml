                            <Button Grid.Column="8" Grid.Row="1" Style="{StaticResource MyButtonStyle}" Click="ALARMA_Click" FontSize="11" FontWeight="Bold" Background="Red">
                                ALARMA
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="5"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Border.Effect>
                                <DropShadowEffect/>
                            </Border.Effect>
                        </Border>
                        <Border  x:Name="btnEnter" Grid.Column="3" Style="{StaticResource BorderIconStyle}">
                            <Button Grid.Column="3" Grid.Row="1" Style="{StaticResource MyButtonStyle}" Click="ENTERBOTON_Click" FontSize="14" FontWeight="Bold" Background="Orange">
                                ENTER
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="5"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Border.Effect>
                                <DropShadowEffect/>
                            </Border.Effect>
                        </Border>
                        <Border  x:Name="btnTurno" Grid.Column="4" Style="{StaticResource BorderIconStyle}">
                            <Button Grid.Column="4" Grid.Row="1" Style="{StaticResource MyButtonStyle}" Click="TURNO_Click" FontSize="14" FontWeight="Bold" Background="Orange">
                                TURNO
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="5"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Border.Effect>
                                <DropShadowEffect/>
                            </Border.Effect>
                        </Border>
                        <Border  x:Name="btnCancel" Grid.Column="5" Style="{StaticResource BorderIconStyle}">
                            <Button Grid.Column="5" Grid.Row="1" Style="{StaticResource MyButtonStyle}" Click="CANCEL_Click" FontSize="14" FontWeight="Bold" Background="LightPink">
                                CANCEL
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="5"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                            <Border.Effect>
                                <DropShadowEffect/>
                            </Border.Effect>
                        </Border>
                    </Grid>
                </Border>
                <Border Style="{StaticResource BorderStyle}" Name="PanelVuelto" Grid.Column="1" Margin="-65 0 5 0" Width="110">
                    <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
                        <TextBlock Name="txtMonto" Style="{StaticResource ValueInferiorStyle}" HorizontalAlignment="Center" Text="R:S/"  Margin="0 5 0 0" FontSize="20"/>
                        <TextBlock Name="txtVuelto" Style="{StaticResource ValueInferiorStyle}" HorizontalAlignment="Center" Text="V:S/"  Margin="0 5 0 0" FontSize="20" />
                    </StackPanel>
                </Border>
            </Grid>
            <Grid Name="PanelInferior" Grid.Row="3" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>
                <TextBlock Name="txtInfoNombre" Style="{StaticResource ValueInferiorStyle}" Margin="10 0 5 0" Text="Último tránsito:"/>
                <TextBlock Name="txtUltimoTransito" Grid.Column="1" Style="{StaticResource LabelInferior3Style}" Text=""/>

                <StackPanel Grid.Column="2" Margin="0,8,0,8" Width="230" Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
                    <TextBlock Name="txtVersionPantalla"  Style="{StaticResource LabelInferior2Style}" TextAlignment="Center" HorizontalAlignment="Center" Text=""/>
                    <TextBlock Name="txtVersionLogica"  Style="{StaticResource LabelInferior2Style}" TextAlignment="Center" HorizontalAlignment="Center" Text=""/>
                </StackPanel>
                <StackPanel Grid.Column="3" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                    <TextBlock Name="txtFecha" Style="{StaticResource ValueSuperiorStyle}" FontWeight="Normal" FontSize="32" Width="160" TextAlignment="Center" HorizontalAlignment="Center" />
                    <TextBlock Name="txtHora" Style="{StaticResource ValueSuperiorStyle}" FontWeight="Bold" FontSize="32" Width="130" TextAlignment="Center" HorizontalAlignment="Center" Margin="5,0,10,0"/>
                </StackPanel>
            </Grid>
        </Grid>

    </Border>



</Window>
